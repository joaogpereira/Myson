<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MysonApplication"
        tools:targetApi="31">
        <activity
            android:name=".SplashActivity"
            android:exported="false" />
        <activity
            android:name=".RelatorioAlimentacaoActivity"
            android:exported="false" />
        <activity
            android:name=".CadastroALimentacaoActivity"
            android:exported="false" />
        <activity
            android:name=".RelatorioSonoActivity"
            android:exported="false" />
        <activity
            android:name=".CadastroSonoActivity"
            android:exported="false" />
        <activity
            android:name=".TelaPerfilActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaPrincipalActivity"
            android:exported="false" />

        <activity
            android:name=".WizardActivity"
            android:exported="false" />
        <activity
            android:name=".CadastroUsuarioActivity"
            android:exported="false" />
        <activity
            android:name=".EsqueciSenhaActivity"
            android:exported="false" />
        <activity
            android:name=".TelaLoginActivity"
            android:exported="false">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>